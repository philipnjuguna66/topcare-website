{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "appsorigin/blogs": "@dev",
        "appsorigin/leads": "@dev",
        "appsorigin/plots": "@dev",
        "appsorigin/teams": "@dev",
        "appsorigin/testimonial": "@dev",
        "bezhansalleh/filament-google-analytics": "^2.0",
        "filament/filament": "^2.0",
        "filament/spatie-laravel-settings-plugin": "^2.0",
        "guava/filament-icon-picker": "^1.3",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/jetstream": "^3.1",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^2.12",
        "maatwebsite/excel": "^3.1",
        "openai-php/client": "^0.7.3",
        "ryangjchandler/filament-navigation": "^0.5.0",
        "spatie/laravel-settings": "^2.8",
        "spatie/laravel-sitemap": "^6.3"
    },
    "require-dev": {
        "doctrine/dbal": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "plugins/leads",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "plugins/projects",
            "options": {
                "symlink": true
            }
        },
        {            "type": "path",
            "url": "plugins/teams",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "plugins/blogs",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "plugins/testimonials",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Utils/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
